@section Title{Investigate players}

@model System.Collections.Generic.IEnumerable<string>

@if (Model.Count() != 0)
{
@:Players in queue: @System.String.Join(", ", Model)
}
else
{
@:No unprocessed players in queue.
}
<form method="POST" action="/Investigate/StartInvestigation">
    Investigate player: <input type="text" name="player"> <input type="submit" value="Investigate">
</form>